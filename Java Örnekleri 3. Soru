package JavaSorular;

public class JavaSoru3 {

	public static void main(String[] args) {
		
		
		int toplama =0;
		int sonuc = 0;
		
		for(int i=45;i<=225;i++)
		{
			if(i%2==0 && i%7==0)
			{
				sonuc=toplama+=i;
			}
			
		}
		System.out.println("45 ile 225 arasında ki 2'ye ve 7'ye tam "+ "\n" + "bölünebilen sayıların toplamı : " +sonuc);

	}

}
